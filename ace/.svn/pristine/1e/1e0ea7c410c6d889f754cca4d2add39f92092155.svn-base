package com.jeefw.model.common;

import java.io.Serializable;
import javax.persistence.*;
import java.math.BigDecimal;


/**
 * The persistent class for the TDEVICEINFO database table.
 * 
 */
@Entity
@NamedQuery(name="Tdeviceinfo.findAll", query="SELECT t FROM Tdeviceinfo t")
public class Tdeviceinfo implements Serializable {
	private static final long serialVersionUID = 1L;

	private String areaguid;

	private String areaname;

	private String daddress;

	private String dcode;

	private String dremark;

	private String dtype;

	private String gprsid;

	private BigDecimal id;

	public Tdeviceinfo() {
	}

	public String getAreaguid() {
		return this.areaguid;
	}

	public void setAreaguid(String areaguid) {
		this.areaguid = areaguid;
	}

	public String getAreaname() {
		return this.areaname;
	}

	public void setAreaname(String areaname) {
		this.areaname = areaname;
	}

	public String getDaddress() {
		return this.daddress;
	}

	public void setDaddress(String daddress) {
		this.daddress = daddress;
	}

	public String getDcode() {
		return this.dcode;
	}

	public void setDcode(String dcode) {
		this.dcode = dcode;
	}

	public String getDremark() {
		return this.dremark;
	}

	public void setDremark(String dremark) {
		this.dremark = dremark;
	}

	public String getDtype() {
		return this.dtype;
	}

	public void setDtype(String dtype) {
		this.dtype = dtype;
	}

	public String getGprsid() {
		return this.gprsid;
	}

	public void setGprsid(String gprsid) {
		this.gprsid = gprsid;
	}

	public BigDecimal getId() {
		return this.id;
	}

	public void setId(BigDecimal id) {
		this.id = id;
	}

}