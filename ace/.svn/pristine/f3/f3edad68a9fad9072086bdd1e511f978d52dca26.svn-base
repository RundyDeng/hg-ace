package com.jeefw.model.common;

import java.io.Serializable;
import javax.persistence.*;
import java.math.BigDecimal;
import java.util.Date;


/**
 * The persistent class for the TAREA database table.
 * 
 */
@Entity
@NamedQuery(name="Tarea.findAll", query="SELECT t FROM Tarea t")
public class Tarea implements Serializable {
	private static final long serialVersionUID = 1L;

	@Temporal(TemporalType.DATE)
	private Date adate;

	private String areacode;

	private BigDecimal areaguid;

	private String areaname;

	private String areaplace;

	@Temporal(TemporalType.DATE)
	private Date bdate;

	private String diagram;

	private BigDecimal factoryno;

	private String gprsid;

	private String imagemap;

	private String imagemap2;

	private String linkman;

	private String nameinjifei;

	private String smemo;

	private String telephone;

	@Column(name="\"YEARS\"")
	private String years;

	public Tarea() {
	}

	public Date getAdate() {
		return this.adate;
	}

	public void setAdate(Date adate) {
		this.adate = adate;
	}

	public String getAreacode() {
		return this.areacode;
	}

	public void setAreacode(String areacode) {
		this.areacode = areacode;
	}

	public BigDecimal getAreaguid() {
		return this.areaguid;
	}

	public void setAreaguid(BigDecimal areaguid) {
		this.areaguid = areaguid;
	}

	public String getAreaname() {
		return this.areaname;
	}

	public void setAreaname(String areaname) {
		this.areaname = areaname;
	}

	public String getAreaplace() {
		return this.areaplace;
	}

	public void setAreaplace(String areaplace) {
		this.areaplace = areaplace;
	}

	public Date getBdate() {
		return this.bdate;
	}

	public void setBdate(Date bdate) {
		this.bdate = bdate;
	}

	public String getDiagram() {
		return this.diagram;
	}

	public void setDiagram(String diagram) {
		this.diagram = diagram;
	}

	public BigDecimal getFactoryno() {
		return this.factoryno;
	}

	public void setFactoryno(BigDecimal factoryno) {
		this.factoryno = factoryno;
	}

	public String getGprsid() {
		return this.gprsid;
	}

	public void setGprsid(String gprsid) {
		this.gprsid = gprsid;
	}

	public String getImagemap() {
		return this.imagemap;
	}

	public void setImagemap(String imagemap) {
		this.imagemap = imagemap;
	}

	public String getImagemap2() {
		return this.imagemap2;
	}

	public void setImagemap2(String imagemap2) {
		this.imagemap2 = imagemap2;
	}

	public String getLinkman() {
		return this.linkman;
	}

	public void setLinkman(String linkman) {
		this.linkman = linkman;
	}

	public String getNameinjifei() {
		return this.nameinjifei;
	}

	public void setNameinjifei(String nameinjifei) {
		this.nameinjifei = nameinjifei;
	}

	public String getSmemo() {
		return this.smemo;
	}

	public void setSmemo(String smemo) {
		this.smemo = smemo;
	}

	public String getTelephone() {
		return this.telephone;
	}

	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}

	public String getYears() {
		return this.years;
	}

	public void setYears(String years) {
		this.years = years;
	}

}