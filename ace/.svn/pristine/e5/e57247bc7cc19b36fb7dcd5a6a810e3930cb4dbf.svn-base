.close-tab {
    font-size: 10px;
    position:absolute;
    right: 5px;
    top: 5px;
    z-index: 100;
    cursor: hand;
    cursor: pointer;
    color: #CB633A;
    display: none;
}

.nav-tabs {position: relative;}
.nav-tabs li{position: relative;}

.iframeClass {
    z-index: 1;
    visibility: inherit;
    width: 100%;
    frameborder: 'no';
    border: 0;
    marginwidth: 0;
    marginheight: 0;
    scrolling-x: 'no';
    scrolling-y: auto;
    allowtransparency: true;
}

.tab-drop {
    margin-right: 30px;
}

.rightMenu {
    position: absolute;
    width: 138px;
    z-index: 99;
    display: none;
}
.list-group-item {
    position: relative;
    display: block;
    padding: 10px 15px;
    margin-bottom: -1px;
    background-color: floralwhite;
    border: 1px solid #ddd;
}